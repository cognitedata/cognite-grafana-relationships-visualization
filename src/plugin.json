{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "panel",
  "name": "Cognite NodeGraph Panel",
  "id": "cognitedata-nodegraph-panel",
  "info": {
    "description": "Panel for visualization of NodeGraph data stored in the Cognite Data Fusion",
    "author": {
      "name": "Cognite",
      "url": "https://cognite.com"
    },
    "keywords": [
      "cognite", "visjs"
    ],
    "logos": {
      "small": "img/CDF_logo.svg",
      "large": "img/CDF_logo.svg"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://github.com/grafana/grafana-starter-panel"
      },
      {
        "name": "License",
        "url": "https://github.com/grafana/grafana-starter-panel/blob/master/LICENSE"
      }
    ],
    "screenshots": [
      {
        "name":"Sample Graph",
        "path":"sample-graph.png"
      }
    ],    
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": ">=7.0.0",
    "plugins": [
      {
        "id":"cognitedata-datasource",
        "name": "Cognite Data Fusion",
        "type":"datasource"
    }]
  }
}
